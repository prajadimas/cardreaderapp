{
  "name": "electron-cardreader",
  "version": "1.1.0",
  "description": "PCSC Card Reader Electron Application",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-rebuild",
    "macbuild": "electron-packager . electron-cardreader --overwrite --platform=darwin --arch=x64 --prune=true --out=.build",
    "winbuild": "electron-packager . electron-cardreader --overwrite --platform=win32 --arch=x64 --prune=true --out=.build",
    "package": "asar pack ./.build/electron-cardreader-darwin-x64/electron-cardreader.app/Contents/Resources/app ./.build/electron-cardreader-darwin-x64/electron-cardreader.app/Contents/Resources/app.asar",
    "setup": "electron-installer-windows --src .build/electron-cardreader-win32-x64/ --dest .build/installers/",
    "start": "electron ."
  },
  "keywords": [
    "Electron",
    "pcsclite",
    "reader",
    "demo"
  ],
  "author": "prajadimas",
  "license": "CC0-1.0",
  "devDependencies": {
    "asar": "^3.0.3",
    "electron": "^9.0.4",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.11.0",
    "electron-winstaller": "^4.0.0"
  },
  "dependencies": {
    "pcsclite": "^1.0.0",
    "ws": "^7.3.0"
  }
}
